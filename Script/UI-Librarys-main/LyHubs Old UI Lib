        MainBorder.AnchorPoint = Vector2.new(0.5, 0.5)
        MainBorder.BackgroundColor3 = Color3.fromRGB(255, 115, 0)
        MainBorder.Position = UDim2.new(0, 226, 0, 127)
        MainBorder.Size = UDim2.new(0, 454, 0, 279)
